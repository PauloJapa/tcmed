<?php

$array = [];

foreach ($this->data as $user) {
    
    /* @var $user \Application\Entity\User */
    $array[$user->getId()] = [
        "id" => $user->getId(),
        "name" => $user->getNome(),
        "type" => "user",
        "status" => $user->getStatusChat()
    ];
}

echo json_encode($array);